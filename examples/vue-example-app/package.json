{
  "name": "yourvictoria-app",
  "version": "1.1.3",
  "private": true,
  "description": "Example Vue.js app(Not Nuxt.js) for showing how to use YourVictoria",
  "scripts": {
    "serve": "vue-cli-service serve --port 3000",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "cy:open": "cypress open",
    "cy:run": "cypress run -e TAGS='not @skip or @smoke'",
    "cy:run-smoke": "cypress run -e TAGS='@smoke' --spec 'test/e2e/integration/smoke/*'",
    "test:dev": "cross-env NODE_ENV=test start-server-and-test serve http://localhost:3000 cy:open",
    "test:smoke": "cross-env TEST=1 start-server-and-test serve http://localhost:3000 cy:run-smoke"
  },
  "dependencies": {
    "@dpc-sdp/ripple-global": "1.8.0",
    "@dpc-sdp/ripple-grid": "1.8.0",
    "@dpc-sdp/ripple-layout": "1.8.0",
    "@dpc-sdp/ripple-site-header": "1.8.0",
    "@dpc-sdp/yourvic-bar-chart": "dev",
    "@dpc-sdp/yourvic-map-core": "dev",
    "core-js": "^3.6.5",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@dpc-sdp/ripple-test-tools": "1.8.0",
    "@dpc-sdp/ripple-card": "1.8.0",
    "@dpc-sdp/ripple-grants": "1.8.0",
    "@vue/cli-plugin-babel": "~4.4.6",
    "@vue/cli-plugin-eslint": "~4.4.6",
    "@vue/cli-service": "~4.4.6",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.3",
    "start-server-and-test": "^1.7.11",
    "svg-sprite-loader": "^5.0.0",
    "svgo": "^1.3.2",
    "svgo-loader": "^2.2.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "step_definitions": "test/e2e/integration/",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "./../../test-results/cucumber",
      "filePrefix": "vue-example-app-"
    }
  }
}
